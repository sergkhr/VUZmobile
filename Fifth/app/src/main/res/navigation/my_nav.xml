            android:id="@+id/action_nickNameSecond_to_nickNameThird"
            app:destination="@id/nickNameThird" />
    </fragment>
    <fragment
        android:id="@+id/nickNameThird"
        android:name="com.example.fifth.view.NickNameThird"
        android:label="fragment_nick_name_third"
